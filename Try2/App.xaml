<Application x:Class="Try2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Try2" xmlns:viewmodel="clr-namespace:Try2.ViewModels">

    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Infrastructure/Commands/AppCommands.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            <viewmodel:ViewModelLocator x:Key="Locator"></viewmodel:ViewModelLocator>

        </ResourceDictionary>

    </Application.Resources>
</Application>
